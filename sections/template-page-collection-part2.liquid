{% for products in section.settings.feature_collection.products %}
    {% render "products" product: products %}
{% else %}
    {% for item in 1..4 %}
        {{ 'product-1' |  placeholder_svg_tag }}
    {% endfor %}
{% endfor %}

{% schema %}
{
    "name": "template page collection part2",
    "settings": [
        {
            "type": "text",
            "id": "title",
            "default": "feature collection",
            "label": "title"
        },
        {
            "type": "collection",
            "id": "feature_collection",
            "label": "Collection"
        }    
    ]
}
{% endschema %}